{
  "name": "tree-sitter-navi-stream",
  "version": "0.0.2",
  "description": "navi-stream grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": ["parsing", "incremental"],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "node-gyp": ">=7 <11",
    "tree-sitter-cli": "^0.20.8"
  },
  "scripts": {
    "build": "tree-sitter generate",
    "build-wasm": "tree-sitter build-wasm",
    "lint": "eslint grammar.js",
    "parse": "tree-sitter parse",
    "test": "bun run build && tree-sitter test",
    "test-windows": "bun run build && tree-sitter test",
    "dev": "bun run build && mkdir -p target && tree-sitter highlight --html examples/*.nvs > target/out.html && open target/out.html"
  },
  "tree-sitter": [
    {
      "scope": "source.navi-stream",
      "injection-regex": "navi-stream",
      "file-types": ["nvs"]
    }
  ]
}
